    223           * @fn      MT_UartProcessZAppData
    224           *
    225           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    226           *          |  1  |  2   |       1         |  1   |
    227           *
    228           *          Parses the data and determine either is SPI or just simply serial data
    229           *          then send the data to correct place (MT or APP)
    230           *
    231           * @param   port    - UART port
    232           *          event   - Event that causes the callback
    233           *
    234           *
    235           * @return  None
    236           ***************************************************************************************************/
    237          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    238          {
    239          
    240            osal_event_hdr_t  *msg_ptr;
    241            uint16 length = 0;
    242            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    243          
    244            /*
    245               If maxZAppBufferLength is 0 or larger than current length
    246               the entire length of the current buffer is returned.
    247            */
    248            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    249            {
    250              length = MT_UartMaxZAppBufLen;
    251            }
    252            else
    253            {
    254              length = rxBufLen;
    255            }
    256          
    257            /* Verify events */
    258            if (event == HAL_UART_TX_FULL)
    259            {
    260              // Do something when TX if full
    261              return;
    262            }
    263          
    264            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    265            {
    266              if ( App_TaskID )
    267              {
    268                /*
    269                   If Application is ready to receive and there is something
    270                   in the Rx buffer then send it up
    271                */
    272                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    273                {
    274                  /* Disable App flow control until it processes the current data */
    275                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    276          
    277                  /* 2 more bytes are added, 1 for CMD type, other for length */
    278                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    279                  if ( msg_ptr )
    280                  {
    281                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    282                    msg_ptr->status = length;
    283          
    284                    /* Read the data of Rx buffer */
    285                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    286          
    287                    /* Send the raw data to application...or where ever */
    288                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    289                  }
    290                }
    291              }
    292            }
    293          }
