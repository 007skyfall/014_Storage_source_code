        MOV     A,R1
        MOV     R4,A
//  175   byte x;
//  176   byte xorResult;
//  177 
//  178   xorResult = 0;
        MOV     R1,#0x0
//  179 
//  180   for ( x = 0; x < len; x++, msg_ptr++ )
